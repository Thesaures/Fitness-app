{
  "compilerOptions": {
    "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
  },
  "extends": "expo/tsconfig.base",
    "include": ["src/**/*"]
  
}
